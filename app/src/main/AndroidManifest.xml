<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parkedapplication">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TicketsClient"></activity>
        <activity android:name=".CreateTicketAgent" />
        <activity android:name=".ProfileAgent" />
        <activity android:name=".ProfileClient" />
        <activity android:name=".Dashboard" />
        <activity android:name=".SignUpClient" />
        <activity android:name=".LoginAgent" />
        <activity android:name=".LoginClient" />
        <activity android:name=".SelectUser" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AgentDashboard" />
        <activity android:name=".OcrCaptureActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>